    <div class="footer-top"></div>
    <div class="footer-wrapper">
        <div class="container">
            <div class="row show-grid">
                <div class="span12">
                    <div class="row show-grid">
                        <!-- FOOTER: LOGO -->
                        @if (ConfigurationManager.AppSettings["Location"] == "Australian")
                        {
                            <div class="span4">
                                <!-- FOOTER: ADDRESS -->
                                <address class="address">
                                    <br /><i class="icon-map-marker icon-large"></i><br />
                                    NeuRA<br />
                                    <i>&nbsp;</i>Margarete Ainsworth Building<br />
                                    <i>&nbsp;</i>Barker Street<br />
                                    <i>&nbsp;</i>Randwick NSW 2031<br />
                                    <i>&nbsp;</i>New Australia<br />
                                    <span class="icon-email"></span><a href="mailto:clinical@hipfracture.com.au">Clinical Support</a><br />
                                    <span class="icon-email"></span><a href="mailto:admin@hipfracture.com.au">Technical Support</a>
                                </address>
                            </div>
                                <!-- FOOTER: ABOUT US -->
                                <div class="span4 footer-center">
                                    <h4 class="center-title">About Us</h4>
                                    <p>The Australian Hip Fracture Registry is produced as a joint project with the Australian & New Zealand Hip Fracture Registry.</p>
                                </div>
                        }
                        else
                        {
                            <div class="span4">
                                <!-- FOOTER: ADDRESS -->
                                <address class="address">
                                    <br /><i class="icon-map-marker icon-large"></i><br />
                                    Health Quality & Safety Commission<br />
                                    <i>&nbsp;</i>Level 8, Customs House<br />
                                    <i>&nbsp;</i>17-21 Whitmore Street<br />
                                    <i>&nbsp;</i>Wellington<br />
                                    <i>&nbsp;</i>New Zealand<br />
                                    <span class="icon-email"></span><a href="mailto:clinical@hipfracture.co.nz">Clinical Support</a><br />
                                    <span class="icon-email"></span><a href="mailto:admin@hipfracture.co.nz">Technical Support</a>
                                </address>
                            </div>
                                <!-- FOOTER: ABOUT US -->
                                <div class="span4 footer-center">
                                    <h4 class="center-title">About Us</h4>
                                    <p>The New Zealand Hip Fracture Registry is produced as a joint project with the Australian & New Zealand Hip Fracture Registry and is proudly co-funded by Health Quality & Safety Commission New Zealand and Osteoporosis New Zealand.</p>
                                </div>
                        }
                        <!-- FOOTER: NAVIGATION LINKS -->
                        <div class="span4 footer-right">
                            <h4 class="center-title">Navigate</h4>
                            <ul class="footer-navigate">
                                <li>
                                    <a href="/index">Home</a>
                                </li>
                                <li>
                                    <a href="/about">About</a>
                                </li>
                                <li>
                                    <a href="/contact">Contact</a>
                                </li>
                                <li>
                                    <a href="/auth/login">Login</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-bottom">
        <div class="container">
            <div class="row show-grid">
                <!-- FOOTER: COPYRIGHT TEXT -->
                <div class="span12">
                    <p>Site built and maintained by <a href="http://www.so3.co">SO³</a>. Copyright 2014</p>
                </div>
            </div>
        </div>
    </div>
</footer>

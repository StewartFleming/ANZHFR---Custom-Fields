<body>
    <!-- THE LINE AT THE VERY TOP OF THE PAGE -->
    <div class="top_line"></div>
    <!-- HEADER AREA -->
    <header>
        <div class="container">
            <div class="row">

                <!-- HEADER: LOGO AREA -->
                <div class="span4 logo">
                    <a class="logo" href="/home/index">
                        <img alt="logo" title="ANZHFR" src="/Content/static/img/logo.png">
                    </a>
                </div>
                @if (ConfigurationManager.AppSettings["Type"] == "Demo")
                {
                    <text>
                        <div class="span4"><h1 style="text-align:center; color:Red; font-weight:bold"> Demonstration Only</h1></div><div class="span4">
                    </text>
                }
                else
                {
                    <text>
                        <div class="span4 offset4">
                    </text>
                }

                <!-- HEADER: PHONE NUMBER -->
                <p class="head_phone">
                    @if (ConfigurationManager.AppSettings["Location"] == "Australian")
                    {
                        <span class="icon-email"></span><a href="mailto:clinical@hipfracture.com.au">Clinical Support</a><br />
                        <span class="icon-email"></span><a href="mailto:admin@hipfracture.com.au">Technical Support</a>
                    }
                    else
                    {
                        <span class="icon-email"></span><a href="mailto:clinical@hipfracture.co.nz">Clinical Support</a><br />
                        <span class="icon-email"></span><a href="mailto:admin@hipfracture.co.nz">Technical Support</a>
                    }
                </p>
            </div>
            <!-- HEADER: SOCIAL ICONS -->
            <!-- <ul class="socials unstyled">
                 <li><a data-rel="tooltip" title="Follow us on Flickr" class="flickr" href="#"></a></li>
                 <li><a data-rel="tooltip" title="Follow us on Twitter" class="twitter" href="#"></a></li>
                 <li><a data-rel="tooltip" title="Follow us on Facebook" class="facebook" href="#"></a></li>
                 <li><a data-rel="tooltip" title="Follow us on Youtube" class="youtube" href="#"></a></li>
                 <li><a data-rel="tooltip" title="Follow us on Dribbble" class="dribbble" href="#"></a></li>
                 <li><a data-rel="tooltip" title="Follow us on Pinterest" class="pinterest" href="#"></a></li>
             </ul> -->
        </div>
    </div>
</header>
<!-- PRIMARY SITE NAVIGATION -->
<div class="navbar-wrapper">
    <div class="container">
        <div class="navbar">
            <div class="navbar-inner">
                <div class="buttons-container">
                </div>
                <ul class="blue nav" id="css3-menu">
                    <li>
                        <a href="/home/index">Home</a>
                    </li>
                    <li>
                        <a href="/home/about">About</a>
                    </li>
                    <li>
                        <a href="/home/contact">Contact Us</a>
                    </li>
                    <li>
                        <a href="/auth/login">Login</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

@RenderBody()
<!-- FOOTER STARTS HERE -->

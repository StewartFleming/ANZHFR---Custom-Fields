    <link rel="stylesheet" type="text/css" href="/Content/static/css/rs-responsive.css" media="screen" />
    <!--  revolution slider plugin : end -->
    <link rel="stylesheet" href="/Content/static/css/bootstrap.css" />
    <link rel="stylesheet" href="/Content/static/css/custom.css" />
    <link rel="stylesheet" href="/Content/static/css/css3-menu.css" />
    <link rel="stylesheet" href="/Content/static/css/isotope.css" />
    <link rel="stylesheet" href="/Content/static/css/color_scheme.css" />
